<?xml version='1.0' encoding='utf-8'?>
<widget id="com.emuchemstock.chemstock" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>EMUChemStock</name>
    <description>
        App for EMU Chemistry Stock Room
    </description>
    <author email="emuchemstock@google.com" href="http://cordova.io">
        Khalob Cognata, Brad Friebe, Andrew Hunn, Cammie Willett, John Zervos
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="Orientation" value="portrait" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-googleplus" spec="~5.1.1">
        <variable name="REVERSED_CLIENT_ID" value="com.googleusercontent.apps.304514613321-vdit6rviu4hdh3kap372vi0kfhvra6s8" />
    </plugin>
</widget>
